{
    "name": "{{name}}",
    "type": "{{type}}",
    "tokenColors": [
      {
        "settings": {
          "background": "{{ui.bg}}",
          "foreground": "{{ui.fg}}"
        }
      },
      {
        "name": "Comment",
        "scope": [
          "comment",
          "punctuation.definition.comment"
        ],
        "settings": {
          "fontStyle": "italic",
          "foreground": "{{scheme.comment}}"
        }
      },
      {
        "name": "Variables",
        "scope": [
          "variable",
          "string constant.other.placeholder"
        ],
        "settings": {
          "foreground": "{{ui.fg}}"
        }
      },
      {
        "name": "Normal",
        "scope": [
          "support.variable.property",
          "meta.jsx.children"
        ],
        "settings": {
          "foreground": "{{ui.fg}}"
        }
      },
      {
        "name": "Colors",
        "scope": [
          "constant.other.color"
        ],
        "settings": {
          "foreground": "{{scheme.base.white}}"
        }
      },
      {
        "name": "Invalid",
        "scope": [
          "invalid",
          "invalid.illegal"
        ],
        "settings": {
          "foreground": "{{scheme.base.red}}"
        }
      },
      {
        "name": "Invalid deprecated",
        "scope": [
          "invalid.deprecated"
        ],
        "settings": {
          "foreground": "{{scheme.base.purple}}"
        }
      },
      {
        "name": "Keyword, Storage",
        "scope": [
          "keyword",
          "storage.type",
          "storage.modifier"
        ],
        "settings": {
          "foreground": "{{scheme.keyword}}"
        }
      },
      {
        "name": "Keyword, Storage",
        "scope": [
          "keyword.control",
          "storage.type",
          "storage.modifier"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "name": "Keyword, Storage",
        "scope": [
          "Keyword",
          "Storage"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "name": "Operator, Misc",
        "scope": [
          "constant.other.color",
          "meta.tag",
          "punctuation.separator.inheritance.php",
          "punctuation.section.embedded",
          "keyword.other.template",
          "keyword.other.substitution",
          "punctuation"
        ],
        "settings": {
          "foreground": "{{scheme.punctuations}}"
        }
      },
      {
        "name": "Tag Start End",
        "scope": [
          "punctuation.definition.tag",
          "punctuation.definition.tag.html",
          "punctuation.definition.tag.begin.html",
          "punctuation.definition.tag.end.html"
        ],
        "settings": {
          "foreground": "{{scheme.punctuations}}"
        }
      },
      {
        "name": "Tag",
        "scope": [
          "entity.name.tag",
          "meta.tag.sgml",
          "markup.deleted.git_gutter",
          "support.class.component"
        ],
        "settings": {
          "foreground": "{{scheme.function}}"
        }
      },
      {
        "name": "JSX Class Tag",
        "scope": [
          "support.class.component"
        ],
        "settings": {
          "fontStyle": "bold"
        }
      },
      {
        "name": "Attributes",
        "scope": [
          "entity.other.attribute-name"
        ],
        "settings": {
          "fontStyle": "italic",
          "foreground": "{{scheme.comment}}"
        }
      },
      {
        "name": "Function, Special Method",
        "scope": [
          "entity.name.function",
          "meta.function-call - variable.other.object",
          "meta.function-call.generic",
          "variable.function",
          "support.function",
          "keyword.other.special-method"
        ],
        "settings": {
          "fontStyle": "bold",
          "foreground": "{{scheme.function}}"
        }
      },
      {
        "name": "C-related Block Level Variables",
        "scope": [
          "source.cpp meta.block variable.other"
        ],
        "settings": {
          "foreground": "{{scheme.base.red}}"
        }
      },
      {
        "name": "Variables constant",
        "scope": [
          "variable.other.constant"
        ],
        "settings": {
          "foreground": "{{scheme.base.pink}}"
        }
      },
      {
        "name": "Other Variable, String Link",
        "scope": [
          "support.other.variable",
          "string.other.link",
          "variable.other.normal.shell",
          "variable.other.positional.shell"
        ],
        "settings": {
          "foreground": "{{scheme.base.pink}}"
        }
      },
      {
        "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
        "scope": [
          "constant.numeric",
          "constant.language",
          "support.constant",
          "constant.character",
          "constant.escape",
          "variable.parameter",
          "keyword.other.unit",
          "keyword.other"
        ],
        "settings": {
          "foreground": "{{scheme.base.pink}}"
        }
      },
      {
        "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
        "scope": [
          "variable.parameter.function.language.special"
        ],
        "settings": {
          "foreground": "{{scheme.base.pink}}"
        }
      },
      {
        "name": "String, Symbols, Inherited Class, Markup Heading",
        "scope": [
          "string",
          "constant.other.symbol",
          "constant.other.key",
          "entity.other.inherited-class",
          "markup.heading",
          "markup.inserted.git_gutter",
          "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
        ],
        "settings": {
          "fontStyle": "normal",
          "foreground": "{{scheme.string}}"
        }
      },
      {
        "name": "Class, Support",
        "scope": [
          "entity.name",
          "support.type",
          "support.class",
          "support.orther.namespace.use.php",
          "meta.use.php",
          "support.other.namespace.php",
          "markup.changed.git_gutter",
          "support.type.sys-types"
        ],
        "settings": {
          "foreground": "{{scheme.class}}"
        }
      },
      {
        "name": "Entity Types",
        "scope": [
          "support.type"
        ],
        "settings": {
          "foreground": "{{scheme.base.paleblue}}"
        }
      },
      {
        "name": "CSS Class and Support",
        "scope": [
          "source.css support.type.property-name",
          "source.sass support.type.property-name",
          "source.scss support.type.property-name",
          "source.less support.type.property-name",
          "source.stylus support.type.property-name",
          "source.postcss support.type.property-name"
        ],
        "settings": {
          "foreground": "{{scheme.class}}"
        }
      },
      {
        "name": "Sub-methods",
        "scope": [
          "entity.name.module.js",
          "variable.import.parameter.js",
          "variable.other.class.js"
        ],
        "settings": {
          "foreground": "{{scheme.base.red}}"
        }
      },
      {
        "name": "Language methods",
        "scope": [
          "variable.language"
        ],
        "settings": {
          "fontStyle": "italic",
          "foreground": "{{scheme.base.red}}"
        }
      },
      {
        "name": "entity.name.method.js",
        "scope": [
          "entity.name.method.js"
        ],
        "settings": {
          "foreground": "{{scheme.base.blue}}"
        }
      },
      {
        "name": "meta.method.js",
        "scope": [
          "meta.class-method.js entity.name.function.js",
          "variable.function.constructor"
        ],
        "settings": {
          "foreground": "{{scheme.base.blue}}"
        }
      },
      {
        "name": "CSS Classes",
        "scope": [
          "entity.other.attribute-name.class"
        ],
        "settings": {
          "foreground": "{{scheme.base.yellow}}"
        }
      },
      {
        "name": "CSS ID's",
        "scope": [
          "source.sass keyword.control"
        ],
        "settings": {
          "foreground": "{{scheme.base.blue}}"
        }
      },
      {
        "name": "Inserted",
        "scope": [
          "markup.inserted"
        ],
        "settings": {
          "foreground": "{{scheme.base.green}}"
        }
      },
      {
        "name": "Deleted",
        "scope": [
          "markup.deleted"
        ],
        "settings": {
          "foreground": "{{scheme.base.red}}"
        }
      },
      {
        "name": "Changed",
        "scope": [
          "markup.changed"
        ],
        "settings": {
          "foreground": "{{scheme.base.purple}}"
        }
      },
      {
        "name": "Regular Expressions",
        "scope": [
          "string.regexp"
        ],
        "settings": {
          "foreground": "{{scheme.string}}"
        }
      },
      {
        "name": "Escape Characters",
        "scope": [
          "constant.character.escape"
        ],
        "settings": {
          "foreground": "{{scheme.base.cyan}}"
        }
      },
      {
        "name": "URL",
        "scope": [
          "*url*",
          "*link*",
          "*uri*"
        ],
        "settings": {
          "fontStyle": "underline"
        }
      },
      {
        "name": "Decorators",
        "scope": [
          "tag.decorator.js entity.name.tag.js",
          "tag.decorator.js punctuation.definition.tag.js"
        ],
        "settings": {
          "fontStyle": "italic",
          "foreground": "{{scheme.base.blue}}"
        }
      },
      {
        "name": "ES7 Bind Operator",
        "scope": [
          "source.js constant.other.object.key.js string.unquoted.label.js"
        ],
        "settings": {
          "fontStyle": "italic",
          "foreground": "{{scheme.base.red}}"
        }
      },
      {
        "name": "JSON Key - Level 0",
        "scope": [
          "source.json meta.structure.dictionary.json support.type.property-name.json",
          "meta.object-literal.key"
        ],
        "settings": {
          "foreground": "{{scheme.jsonEven}}"
        }
      },
      {
        "name": "JSON Key - Level 1",
        "scope": [
          "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
